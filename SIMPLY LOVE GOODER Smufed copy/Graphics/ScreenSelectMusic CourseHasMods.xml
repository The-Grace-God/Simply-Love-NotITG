<ActorFrame> <children>
	<Layer
		File="longverbubble.png"
		OnCommand="%function(self) self:rotationz(180) ColorRGBCommand(self, -1) self:zoom(0.5) self:x(-110) self:y(-130) end" 
	/>
	<Layer
		Type="BitmapText"
		Text="COURSE HAS MODS"
		File="_misobold small"
		InitCommand="zoom,.5;shadowlength,0;addx,-2-110;addy,-10-130"
		OnCommand="%function(self) self:diffuse(TextOnColor(-1)) end"
		OffCommand="linear,0.2;diffusealpha,0"
	/>
</children> </ActorFrame>