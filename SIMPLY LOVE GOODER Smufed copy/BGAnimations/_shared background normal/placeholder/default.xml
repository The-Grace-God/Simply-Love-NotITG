<Layer 
		Type="Quad"
		OnCommand="sleep,0.02;queuecommand,SetMe"
		SetMeCommand="%function(self)
		self:zoomto(SCREEN_WIDTH*2,SCREEN_HEIGHT,0,0)
		self:diffuse(0,0,0,0.5)
		self:x(SCREEN_CENTER_X)
		self:y(SCREEN_CENTER_Y)
		end"
	/>


	<Layer File="idle.sprite"
		
		OnCommand="sleep,0.02;queuecommand,SetMe"
		SetMeCommand="%function(self)
		self:horizalign('right')
		self:x(SCREEN_RIGHT-100)
		self:y(SCREEN_CENTER_Y)
		self:zoom(0.5)
		self:bob()
		self:effectmagnitude(0,5,0)
		end"
	/>
