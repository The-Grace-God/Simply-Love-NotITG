        <AutoActor
        Type="BitmapText"
        Text="New Feature::::Change/Fix"
        Font="Common Normal"
        OnCommand="%function(self)
        self:x(SCREEN_WIDTH*1.4)
        self:horizalign('right')
        self:y(SCREEN_CENTER_Y+100)
        self:zoom(0.5)
        end"
        OffCommand="linear,0.3;diffusealpha,0"
        />

        <AutoActor
        Type="Quad"
        OnCommand="%function(self)
        self:x(SCREEN_WIDTH*1.28)
        self:horizalign('right')
        self:y(SCREEN_CENTER_Y+90)
        self:zoom(10)
        self:diffuse(0,1,0,0)
        self:linear(1)
        self:diffusealpha(1)
        self:diffuse(0,1,0,1)
        end"
        OffCommand="linear,0.3;diffusealpha,0"
        />

        <AutoActor
        Type="Quad"
        OnCommand="%function(self)
        self:x(SCREEN_WIDTH*1.28)
        self:horizalign('right')
        self:y(SCREEN_CENTER_Y+125)
        self:zoom(10)
        self:diffuse(1,0.5,0,0)
        self:linear(1)
        self:diffusealpha(1)
        self:diffuse(1,0.5,0,1)
        end"
